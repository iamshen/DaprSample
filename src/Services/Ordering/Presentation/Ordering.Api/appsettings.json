      {
        "Title": "Mobile API （移动平台）",
        "Name": "mobile"
      },
      {
        "Title": "Open API （公共开放）",
        "Name": "open"
      }
    ],
    "RoutePrefix": "docs",
    "IsHideSchemas": false,
    "Enabled": true
  }
}
