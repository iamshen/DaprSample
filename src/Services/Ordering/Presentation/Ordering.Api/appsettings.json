      "Override": {
        "Microsoft": "Warning",
        "DaprTool.BuildingBlocks": "Information",
        "System": "Warning"
      }
    }
  },
  "SeqServerUrl": null,
  "AllowedHosts": "*",
  "SwaggerOptions": {
    "Endpoints": [
      {
        "Title": "Default",
