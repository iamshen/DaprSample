@inject HttpConfiguration HttpConfiguration

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="@HttpConfiguration.BasePath" />
    <link rel="stylesheet" href="_content/WebAdmin.Shared/css/app.css" />
    <link rel="stylesheet" href="_content/Microsoft.FluentUI.AspNetCore.Components/css/reboot.css" />
    <link rel="stylesheet" href="WebAdmin.styles.css" />
